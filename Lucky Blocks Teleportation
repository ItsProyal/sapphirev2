	print("Teleported to ORANGE")
end)


PinkLoc.MouseButton1Click:Connect(function()
	game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-773.309875, 193.300201, 205.160767)
	print("Teleported to PINK")
end)







GreenLoc.MouseButton1Click:Connect(function()
	print("Teleported to GREEN")
	game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-1154.34985, 193.793961, -177.970261)
end)







GotoPlayer.MouseButton1Click:Connect(function()
	game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Workspace[PlayerNameValue.Text].HumanoidRootPart.CFrame
end)

