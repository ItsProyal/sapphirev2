-- Scripts:

RedLoc.MouseButton1Click:Connect(function()
	game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-1155.27527, 193.300201, 358.762299)
	print("Teleported to RED")
end)

PurpleLoc.MouseButton1Click:Connect(function()
	game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-927.073853, 193.300201, 358.762299)
	print("Teleported to PURPLE")
end)

BlueLoc.MouseButton1Click:Connect(function()
	game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-1309.27844, 193.300201, 205.191528)
	print("Teleported to BLUE")
end)

LBlueLoc.MouseButton1Click:Connect(function()
	game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-1309.07849, 193.300201, -23.108427)
	print("Teleported to LIGHT BLUE")
end)

YellowLoc.MouseButton1Click:Connect(function()
